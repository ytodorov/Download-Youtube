    <div class="content-wrap bg-color14">
        <section id="Home" class="pt-section" data-name="@Resource.Home">
            <div class="container">
                <div class="text-center color--youtube">
                    <h1 class="font-regular-bold hovercolor"><a href="/" target="_self" class="ahome">Download YouTube</a></h1>
                </div>
                <div class="text-center color--youtube">
                    <h2 class="font-regular-normal hovercolor">@Resource.SiteTitle.Replace(".", string.Empty)</h2>
                </div>
            </div>
        </section>
        <div id="Social" class="pt-section" data-name="@Resource.FollowUs">
            @*<h4 style="display:none">@Resource.FollowUs</h4>*@

            <div class="container">
                <div class="row">
                    @*<div class="col-sm-2"></div>*@
                    <div class="social-share col-sm-12 headerSocial">
                        <div class="fb-like"
                             data-href="https://www.facebook.com/DownloadYouTubeOrg"
                             data-colorscheme="light"
                             data-layout="button_count"
                             data-action="like"
                             data-size="small"
                             data-show-faces="false"
                             data-share="true">
                        </div>
                        <a class="twitter-follow-button"
                           style="vertical-align:bottom"
                           href="https://twitter.com/DownloadYouT"
                           data-show-screen-name="false">
                        </a>
                        <a class="twitter-share-button"
                           href="https://twitter.com/intent/tweet?url=http%3A%2F%2Fdownload-youtube.org"
                           data-show-screen-name="false">
                            Tweet
                        </a>
                        <div class="g-follow" data-size="medium" data-annotation="inline" data-width="200" data-href="https://plus.google.com/116818711365591733192" data-rel="author"></div>
                        <div class="g-plusone" data-size="medium" data-annotation="inline" data-width="200"></div>
                    </div>
                </div>
            </div>

        </div>
        <div class="menu-button nav-icon" id="open-button">
            <div></div>
        </div>

        @*Yordan*@
        @RenderBody()



        <div id="content">
            <section id="info" class="pt-section" data-name="@Resource.Info">
                <div class="container dark">
                    <div class="row features">
                        <div class="col-xs-12 col-sm-6 col-md-4">
                            <div class="feature">
                                <h3><i class="pe-7s-medal"></i>&nbsp; @Resource.SupportMultipleFormatsTitle</h3>
                                <p class="text-justify">@Resource.SupportMultipleFormatsDescription</p>
                            </div>
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-4">
                            <div class="feature">
                                <h3><i class="pe-7s-tools"></i>&nbsp; @Resource.WorksOnAllPlatformsTitle</h3>
                                <p class="text-justify">@Resource.WorksOnAllPlatformsDescription</p>
                            </div>
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-4">
                            <div class="feature">
                                <h3> <i class="pe-7s-cloud-upload"></i>&nbsp; @Resource.ExtractionInTheCloudTitle</h3>
                                <p class="text-justify">@Resource.ExtractionInTheCloudDescription</p>
                            </div>
                        </div>
                    </div>
                </div>
            </section>



